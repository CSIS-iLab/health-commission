{
  "name": "csis-jekyll-starter",
  "version": "0.0.0",
  "description": "A Jekyll starter kit for CSIS iDeas Lab projects.",
  "main": "gulpfile.js",
  "scripts": {
    "preinstall": "bundle install --path vendor/bundle",
    "start": "bundle exec gulp",
    "test": "bundle exec gulp test",
    "browsersync": "bundle exec gulp browsersync",
    "browsersync-production": "bundle exec gulp browsersync --jekyllEnv='production'",
    "build": "bundle exec gulp build --jekyllEnv='production'",
    "eslint": "bundle exec gulp eslint",
    "imagemin": "bundle exec gulp imagemin",
    "production": "bundle exec gulp --jekyllEnv='production'",
    "sass": "bundle exec gulp sass",
    "watch": "bundle exec gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CSIS-iLab/csis-jekyll-starter.git"
  },
  "keywords": [
    "Autoprefixer",
    "Browsersync",
    "gulp",
    "imagemin",
    "Jekyll",
    "PostCSS",
    "Sass",
    "Webpack"
  ],
  "author": "CSIS iDeas Lab",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "^9.1.0",
    "browser-sync": "^2.24.6",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-prettier": "^2.6.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-watch": "^5.0.1",
    "imagemin-pngquant": "^6.0.0",
    "prettier": "^1.14.2",
    "require-dir": "^1.0.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^4.17.2",
    "webpack-stream": "^5.1.1",
    "yargs": "^12.0.1"
  }
}
